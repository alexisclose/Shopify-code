{"sections":{"eaff04b0-1711-4865-92f4-2c27abdb9e73":{"type":"main-page","disabled":true,"settings":{"padding_top":36,"padding_bottom":36}},"rich_text_BAjDPj":{"type":"rich-text","blocks":{"heading_8tzwgQ":{"type":"heading","settings":{"heading":"<em>The Spirit Atlas<\/em>","heading_size":"h1"}},"text_rFbjFy":{"type":"text","settings":{"text":"<p>Learn, Taste, Compare. Enjoy an interactive experience tailored to your El Cartel Tasting Box.<\/p>"}},"button_6rbhwW":{"type":"button","disabled":true,"settings":{"button_label":"English","button_link":"https:\/\/padlet.com\/elcartel\/the-single-malt-whisky-tour-c7lql9p0onx5rnf4","button_style_secondary":false,"button_label_2":"Nederlands","button_link_2":"https:\/\/form.typeform.com\/to\/jAKFewnF","button_style_secondary_2":false}},"button_NUGhDU":{"type":"button","disabled":true,"settings":{"button_label":"Deutsch","button_link":"","button_style_secondary":false,"button_label_2":"Français","button_link_2":"","button_style_secondary_2":false}}},"block_order":["heading_8tzwgQ","text_rFbjFy","button_6rbhwW","button_NUGhDU"],"name":"t:sections.rich-text.presets.name","settings":{"desktop_content_position":"center","content_alignment":"center","color_scheme":"scheme-4","full_width":true,"padding_top":60,"padding_bottom":0}},"custom_liquid_EEacrH":{"type":"custom-liquid","name":"t:sections.custom-liquid.presets.name","settings":{"custom_liquid":"<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Tasting Set Selector<\/title>\n    <style>\n        \/* Main container for the selector section *\/\n        .custom-selector-section {\n            background-color: #121212; \/* Updated background color *\/\n            padding: 40px 20px;\n            font-family: serif;\n            color: grey;\n            text-align: center;\n            display: flex;\n            flex-direction: column;\n            justify-content: center;\n        }\n\n        \/* Label for each step, e.g., \"Choose your set:\" *\/\n        .step-label {\n            font-size: 20px;\n            margin-bottom: 20px;\n            color: white; \/* Set label text color to white *\/\n        }\n\n        \/* Flex container for the buttons *\/\n        .button-group {\n            display: flex;\n            flex-wrap: wrap;\n            justify-content: center;\n            gap: 15px;\n        }\n\n        \/* Styling for the individual option buttons *\/\n        .option-button {\n            background-color: white;\n            color: black;\n            border: none;\n            padding: 15px 20px;\n            font-size: 16px;\n            font-family: serif; \/* Set button font to serif *\/\n            border-radius: 9999px; \/* Creates the pill shape *\/\n            cursor: pointer;\n            min-width: 220px;\n            transition: transform 0.2s ease;\n        }\n\n        \/* Hover effect for buttons *\/\n        .option-button:hover {\n            transform: scale(1.05);\n        }\n\n        \/* Responsive adjustments for smaller screens *\/\n        @media (max-width: 768px) {\n            .language-buttons {\n                flex-direction: column;\n                align-items: center;\n            }\n\n            .option-button {\n                width: 100%;\n                max-width: 300px;\n            }\n        }\n    <\/style>\n<\/head>\n<body>\n\n<div class=\"custom-selector-section\">\n    <!-- Step 1: Language Selection -->\n    <div class=\"step\" id=\"languageStep\">\n        <p class=\"step-label\">Choose your language:<\/p>\n        <div class=\"button-group language-buttons\">\n            <button class=\"option-button\" onclick=\"showSets('en')\">English<\/button>\n            <button class=\"option-button\" onclick=\"showSets('nl')\">Nederlands<\/button>\n            <button class=\"option-button\" onclick=\"showSets('fr')\">Français<\/button>\n            <button class=\"option-button\" onclick=\"showSets('de')\">Deutsch<\/button>\n        <\/div>\n    <\/div>\n\n    <!-- Step 2: Set Selection (initially hidden) -->\n    <div class=\"step\" id=\"setStep\" style=\"display:none;\">\n        <p class=\"step-label\" id=\"set-step-label\">Choose your set:<\/p>\n        <div class=\"button-group set-buttons\">\n            <button class=\"option-button\" onclick=\"goToPage('scotch-12')\" data-set-id=\"scotch-12\">Scotch\/Irish Whisky Tasting Set 12 Tubes<\/button>\n            <button class=\"option-button\" onclick=\"goToPage('scotch-6')\" data-set-id=\"scotch-6\">Scotch\/Irish Whisky Tasting Set 6 Tubes<\/button>\n            <button class=\"option-button\" onclick=\"goToPage('japanese-12')\" data-set-id=\"japanese-12\">Japanese Whisky Tasting Set 12 Tubes<\/button>\n            <button class=\"option-button\" onclick=\"goToPage('japanese-6')\" data-set-id=\"japanese-6\">Japanese Whisky Tasting Set 6 Tubes<\/button>\n        <\/div>\n    <\/div>\n<\/div>\n\n<script>\n    \/\/ Variable to store the chosen language\n    let selectedLanguage = '';\n\n    \/\/ ---===[ TRANSLATIONS FOR SET NAMES ]===---\n    const translations = {\n        'en': {\n            'choose-set': 'Choose your set:',\n            'scotch-12': 'Scotch\/Irish Whisky Tasting Set 12 Tubes',\n            'scotch-6': 'Scotch\/Irish Whisky Tasting Set 6 Tubes',\n            'japanese-12': 'Japanese Whisky Tasting Set 12 Tubes',\n            'japanese-6': 'Japanese Whisky Tasting Set 6 Tubes'\n        },\n        'nl': {\n            'choose-set': 'Kies je set:',\n            'scotch-12': 'Schotse\/Ierse Whisky Proeverij Set 12 Buisjes',\n            'scotch-6': 'Schotse\/Ierse Whisky Proeverij Set 6 Buisjes',\n            'japanese-12': 'Japanse Whisky Proeverij Set 12 Buisjes',\n            'japanese-6': 'Japanse Whisky Proeverij Set 6 Buisjes'\n        },\n        'fr': {\n            'choose-set': 'Choisissez votre coffret :',\n            'scotch-12': 'Coffret Dégustation Whisky Écossais\/Irlandais 12 Tubes',\n            'scotch-6': 'Coffret Dégustation Whisky Écossais\/Irlandais 6 Tubes',\n            'japanese-12': 'Coffret Dégustation Whisky Japonais 12 Tubes',\n            'japanese-6': 'Coffret Dégustation Whisky Japonais 6 Tubes'\n        },\n        'de': {\n            'choose-set': 'Wählen Sie Ihr Set:',\n            'scotch-12': 'Schottischer\/Irischer Whisky Verkostungsset 12 Röhrchen',\n            'scotch-6': 'Schottischer\/Irischer Whisky Verkostungsset 6 Röhrchen',\n            'japanese-12': 'Japanischer Whisky Verkostungsset 12 Röhrchen',\n            'japanese-6': 'Japanischer Whisky Verkostungsset 6 Röhrchen'\n        }\n    };\n    \/\/ ---===[ END OF TRANSLATIONS ]===---\n\n    \/\/ ---===[ EDIT YOUR PADLET URLS HERE ]===---\n    const padletUrls = {\n        'en': {\n            'scotch-12': 'https:\/\/padlet.com\/your-username\/english-scotch-12',\n            'scotch-6': 'https:\/\/padlet.com\/your-username\/english-scotch-6',\n            'japanese-12': 'https:\/\/padlet.com\/your-username\/english-japanese-12',\n            'japanese-6': 'https:\/\/padlet.com\/your-username\/english-japanese-6'\n        },\n        'nl': {\n            'scotch-12': 'https:\/\/padlet.com\/your-username\/dutch-scotch-12',\n            'scotch-6': 'https:\/\/padlet.com\/your-username\/dutch-scotch-6',\n            'japanese-12': 'https:\/\/padlet.com\/your-username\/dutch-japanese-12',\n            'japanese-6': 'https:\/\/padlet.com\/your-username\/dutch-japanese-6'\n        },\n        'fr': {\n            'scotch-12': 'https:\/\/padlet.com\/your-username\/french-scotch-12',\n            'scotch-6': 'https:\/\/padlet.com\/your-username\/french-scotch-6',\n            'japanese-12': 'https:\/\/padlet.com\/your-username\/french-japanese-12',\n            'japanese-6': 'https:\/\/padlet.com\/your-username\/french-japanese-6'\n        },\n        'de': {\n            'scotch-12': 'https:\/\/padlet.com\/your-username\/german-scotch-12',\n            'scotch-6': 'https:\/\/padlet.com\/your-username\/german-scotch-6',\n            'japanese-12': 'https:\/\/padlet.com\/your-username\/german-japanese-12',\n            'japanese-6': 'https:\/\/padlet.com\/your-username\/german-japanese-6'\n        }\n    };\n    \/\/ ---===[ END OF URLS ]===---\n\n\n    \/\/ Function to store the language, update text, and show the set buttons\n    function showSets(language) {\n        selectedLanguage = language; \/\/ Store the selected language\n        \n        const setTranslations = translations[language];\n\n        \/\/ Update the \"Choose your set:\" label\n        document.getElementById('set-step-label').textContent = setTranslations['choose-set'];\n\n        \/\/ Update the text on each set button\n        const setButtons = document.querySelectorAll('.set-buttons .option-button');\n        setButtons.forEach(button => {\n            const setId = button.dataset.setId;\n            if (setTranslations[setId]) {\n                button.textContent = setTranslations[setId];\n            }\n        });\n\n        \/\/ Show the next step\n        document.getElementById('languageStep').style.display = 'none';\n        document.getElementById('setStep').style.display = 'block';\n    }\n\n    \/\/ Function to navigate to a unique product page based on language and set\n    function goToPage(setId) {\n        if (!selectedLanguage) {\n            console.error(\"Language has not been selected.\");\n            document.getElementById('setStep').style.display = 'none';\n            document.getElementById('languageStep').style.display = 'block';\n            return;\n        }\n\n        const url = padletUrls[selectedLanguage]?.[setId];\n        \n        if (url) {\n            window.location.href = url;\n            console.log(`Navigating to: ${url}`);\n        } else {\n            console.error(`No Padlet URL found for language '${selectedLanguage}' and set '${setId}'.`);\n            alert(\"Sorry, a link for this combination is not available.\");\n        }\n    }\n<\/script>\n\n<\/body>\n<\/html>","color_scheme":"scheme-3","padding_top":0,"padding_bottom":0}},"ss_steps_3_YYdabw":{"type":"ss-steps-3","blocks":{"step_nT4WNL":{"type":"step","settings":{"title":"Learn about each whisky","text":""}},"step_gzqGHy":{"type":"step","settings":{"title":"Compare your tasting notes with others","text":""}},"step_Q83itJ":{"type":"step","settings":{"title":"Be part of The El Cartel community","text":""}}},"block_order":["step_nT4WNL","step_gzqGHy","step_Q83itJ"],"name":"SS - Steps #3","settings":{"heading":"<h1>How it works <\/h1>","heading_custom":false,"heading_font":"serif","heading_size":34,"heading_size_mobile":20,"heading_height":130,"heading_align":"center","heading_align_mobile":"center","body_gap":24,"body_gap_mobile":20,"body_mt":48,"body_mt_mobile":16,"content_maxwidth":320,"content_align":"center","number_width":92,"number_width_mobile":52,"number_border_thickness":2,"number_custom":false,"number_font":"serif","number_size":48,"number_size_mobile":24,"number_height":100,"title_custom":false,"title_font":"serif","title_size":24,"title_size_mobile":16,"title_height":100,"title_mt":32,"title_mt_mobile":0,"text_custom":false,"text_font":"serif","text_size":18,"text_size_mobile":12,"text_height":130,"text_mt":16,"text_mt_mobile":8,"steps_border_color":"#003d29","number_border_color":"#003d29","number_bg_color":"#f3f3f3","number_color":"#003d29","title_color":"#f3f3f3","text_color":"#f3f3f3","heading_color":"#f3f3f3","background_color":"#071a6b","background_gradient":"linear-gradient(256deg, rgba(0, 0, 0, 1) 14%, rgba(18, 45, 73, 1) 58%)","border_color":"#000000","margin_top":0,"margin_bottom":0,"padding_top":64,"padding_bottom":64,"padding_horizontal":5,"padding_horizontal_mobile":1.5,"full_width":false,"content_width":140,"border_thickness":0}},"custom_liquid_YAQhXk":{"type":"custom-liquid","disabled":true,"settings":{"custom_liquid":"","color_scheme":"",