        "custom_liquid_kHkkHE": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "<script>\n// Comprehensive cart debugging\n(function() {\n  console.log('üîç Cart debugger loaded');\n  \n  // Method 1: Intercept form submission\n  document.addEventListener('submit', function(e) {\n    if (e.target.matches('form[action*=\"/cart/add\"]')) {\n      console.log('üìã FORM SUBMIT DETECTED');\n      console.log('Form:', e.target);\n      \n      const formData = new FormData(e.target);\n      console.log('=== Form Data Being Sent ===');\n      for (let [key, value] of formData.entries()) {\n        console.log('  ' + key + ':', value);\n      }\n      \n      const variantId = formData.get('id');\n      console.log('üéØ Variant ID being added:', variantId);\n      \n      // Look up what this variant should be\n      fetch(window.location.pathname + '.js')\n        .then(r => r.json())\n        .then(p => {\n          const v = p.variants.find(x => x.id == variantId);\n          if (v) {\n            console.log('üì¶ This variant is:', v.title, '- Price:', v.price / 100);\n          }\n        });\n    }\n  }, true);\n  \n  // Method 2: Intercept fetch/AJAX requests\n  const originalFetch = window.fetch;\n  window.fetch = function(...args) {\n    const url = args[0];\n    \n    if (typeof url === 'string' && url.includes('/cart/add')) {\n      console.log('üåê AJAX CART ADD DETECTED');\n      console.log('URL:', url);\n      \n      if (args[1] && args[1].body) {\n        console.log('Body:', args[1].body);\n        \n        // If it's FormData\n        if (args[1].body instanceof FormData) {\n          for (let [key, value] of args[1].body.entries()) {\n            console.log('  ' + key + ':', value);\n          }\n        }\n        // If it's JSON\n        else if (typeof args[1].body === 'string') {\n          try {\n            const data = JSON.parse(args[1].body);\n            console.log('JSON data:', data);\n            console.log('üéØ Variant ID in AJAX:', data.id);\n          } catch(e) {}\n        }\n      }\n    }\n    \n    return originalFetch.apply(this, args);\n  };\n  \n  // Method 3: Intercept XMLHttpRequest\n  const originalXHR = window.XMLHttpRequest;\n  window.XMLHttpRequest = function() {\n    const xhr = new originalXHR();\n    const originalSend = xhr.send;\n    \n    xhr.send = function(data) {\n      if (xhr._url && xhr._url.includes('/cart/add')) {\n        console.log('üì° XHR CART ADD DETECTED');\n        console.log('Data:', data);\n      }\n      return originalSend.apply(this, arguments);\n    };\n    \n    const originalOpen = xhr.open;\n    xhr.open = function(method, url) {\n      xhr._url = url;\n      return originalOpen.apply(this, arguments);\n    };\n    \n    return xhr;\n  };\n})();\n</script>"
          }
        },
