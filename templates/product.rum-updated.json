lect = findSizeSelect();\n    if (!sizeSelect) return;\n    var size = sizeSelect.value;\n    var variant = productData.variants.find(function(v) { return v.option1 === size; });\n    if (variant) {\n      selectedVariantId = variant.id;\n      var priceString = '‚Ç¨' + (variant.price / 100).toFixed(2).replace('.', ',');\n      var mainPrice = document.querySelector('.price-item--regular, .price__regular .price-item__regular');\n      if (mainPrice) mainPrice.innerHTML = '<span class=\"money\">' + priceString + '</span>';\n      var addBtn = document.querySelector('button[name=\"add\"], .product-form__submit');\n      if (addBtn) {\n        addBtn.disabled = !variant.available;\n        var btnText = addBtn.querySelector('span') || addBtn;\n        if (variant.available) {\n          btnText.textContent = t.addToCart;\n        } else {\n          btnText.textContent = t.soldOut;\n        }\n      }\n    }\n  }\n\n  document.body.addEventListener('click', function(e) {\n    var btn = e.target.closest('button[name=\"add\"], .product-form__submit, form[action*=\"/cart/add\"] button');\n    if (btn && selectedVariantId) {\n      e.preventDefault();\n      e.stopPropagation();\n      e.stopImmediatePropagation();\n      var originalText = btn.innerText;\n      btn.innerText = t.adding;\n      btn.disabled = true;\n      var formData = { 'items': [{ 'id': selectedVariantId, 'quantity': 1 }] };\n      fetch(window.Shopify.routes.root + 'cart/add.js', {\n        method: 'POST',\n        headers: { 'Content-Type': 'application/json' },\n        body: JSON.stringify(formData)\n      })\n      .then(function(r) { return r.json(); })\n      .then(function(data) { window.location.href = '/cart'; })\n      .catch(function(err) {\n        btn.innerText = originalText;\n        btn.disabled = false;\n        alert(t.error);\n      });\n    }\n  }, true);\n})();\n</script>"
