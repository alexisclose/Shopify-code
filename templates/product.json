          "settings": {
            "custom_liquid": "{% assign today_wday = 'now' | date: \"%w\" | plus: 0 %}\n{% assign today_ts = 'now' | date: \"%s\" | plus: 0 %}\n\n{% comment %} Calculate shipping date (1-2 business days) {% endcomment %}\n{% case today_wday %}\n  {% when 0 %} {% assign ship_days = 1 %}\n  {% when 1 %} {% assign ship_days = 1 %}\n  {% when 2 %} {% assign ship_days = 1 %}\n  {% when 3 %} {% assign ship_days = 1 %}\n  {% when 4 %} {% assign ship_days = 1 %}\n  {% when 5 %} {% assign ship_days = 3 %}\n  {% when 6 %} {% assign ship_days = 2 %}\n{% endcase %}\n\n{% comment %} Calculate delivery date (1 day after shipping) {% endcomment %}\n{% assign ship_seconds = ship_days | times: 86400 %}\n{% assign ship_timestamp = today_ts | plus: ship_seconds %}\n{% assign deliver_seconds = 86400 %}\n{% assign deliver_timestamp = ship_timestamp | plus: deliver_seconds %}\n\n{% assign ship_date = ship_timestamp | date: \"%d %B\" %}\n{% assign deliver_date = deliver_timestamp | date: \"%d %B\" %}\n\n<style>\n.shipping-timeline-wrapper {\n  padding-top: 25px;\n}\n\n.shipping-timeline {\n  display: flex;\n  align-items: stretch;\n  background: #fff;\n  border-radius: 12px;\n  overflow: hidden;\n  box-shadow: 0 2px 8px rgba(0,0,0,0.08);\n  width: 100%;\n  max-width: 450px;\n  margin: 0 auto 20px auto;\n}\n\n.timeline-step {\n  flex: 1;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  padding: 16px 8px;\n  text-align: center;\n  min-width: 0;\n}\n\n.timeline-step.active {\n  background: #6B3541;\n  color: #fff;\n  border-radius: 10px;\n}\n\n.timeline-step.active .step-label {\n  color: #fff;\n}\n\n.timeline-step.active .step-date {\n  color: rgba(255,255,255,0.9);\n}\n\n.step-icon {\n  width: 28px;\n  height: 28px;\n  margin-bottom: 8px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.step-icon svg {\n  width: 24px;\n  height: 24px;\n  fill: currentColor;\n}\n\n.timeline-step:not(.active) .step-icon svg {\n  fill: #6B3541;\n}\n\n.step-label {\n  font-size: 14px;\n  font-weight: 600;\n  color: #1a1a1a;\n  margin-bottom: 2px;\n  white-space: nowrap;\n}\n\n.step-date {\n  font-size: 12px;\n  color: #666;\n  white-space: nowrap;\n}\n</style>\n\n<div class=\"shipping-timeline-wrapper\">\n  <div class=\"shipping-timeline\">\n    <div class=\"timeline-step active\">\n      <div class=\"step-icon\">\n        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\">\n          <path d=\"M19 3H5c-1.1 0-2 .9-2 2v14c0 1.1.9 2 2 2h14c1.1 0 2-.9 2-2V5c0-1.1-.9-2-2-2zm-7 14l-5-5 1.41-1.41L12 14.17l4.59-4.58L18 11l-6 6z\"/>\n        </svg>\n      </div>\n      <span class=\"step-label\">Prepared</span>\n      <span class=\"step-date\">Today</span>\n    </div>\n    \n    <div class=\"timeline-step\">\n      <div class=\"step-icon\">\n        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\">\n          <path d=\"M20 8h-3V4H3c-1.1 0-2 .9-2 2v11h2c0 1.66 1.34 3 3 3s3-1.34 3-3h6c0 1.66 1.34 3 3 3s3-1.34 3-3h2v-5l-3-4zM6 18.5c-.83 0-1.5-.67-1.5-1.5s.67-1.5 1.5-1.5 1.5.67 1.5 1.5-.67 1.5-1.5 1.5zm13.5-9l1.96 2.5H17V9.5h2.5zm-1.5 9c-.83 0-1.5-.67-1.5-1.5s.67-1.5 1.5-1.5 1.5.67 1.5 1.5-.67 1.5-1.5 1.5z\"/>\n        </svg>\n      </div>\n      <span class=\"step-label\">Shipping</span>\n      <span class=\"step-date\">{{ ship_date }}</span>\n    </div>\n    \n    <div class=\"timeline-step\">\n      <div class=\"step-icon\">\n        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\">\n          <path d=\"M12 2C8.13 2 5 5.13 5 9c0 5.25 7 13 7 13s7-7.75 7-13c0-3.87-3.13-7-7-7zm0 9.5c-1.38 0-2.5-1.12-2.5-2.5s1.12-2.5 2.5-2.5 2.5 1.12 2.5 2.5-1.12 2.5-2.5 2.5z\"/>\n        </svg>\n      </div>\n      <span class=\"step-label\">Delivered</span>\n      <span class=\"step-date\">{{ deliver_date }}</span>\n    </div>\n  </div>\n</div>"
